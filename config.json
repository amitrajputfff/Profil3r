{
    "config_path": "./config.json",
    "separators": {
        "Dot": ".",
        "Dash": "-",
        "Underscore": "_"
    },
    "report_elements": [
        "email",
        "facebook",
        "twitter"
    ],
    "json_report_path": "./reports/json/{}.json",
    "html_report_path": "./reports/html/{}.html",
    "csv_report_path": "./reports/csv/{}.csv",
    "plateform": {
        "domain": {
            "rate_limit": 100,
            "format": "http://{permutation}.{domain}",
            "TLD": [
                "org",
                "com",
                "net"
            ],
            "type": "domain"
        },
        "email": {
            "rate_limit": 1500,
            "domains": [
                "gmail.com",
                "yahoo.com",
                "hotmail.com"
            ],
            "format": "{permutation}@{domain}",
            "type": "email"
        },
        "facebook": {
            "rate_limit": 1000,
            "format": "https://facebook.com/{permutation}",
            "type": "social"
        },
        "twitter": {
            "rate_limit": 1000,
            "format": "https://twitter.com/{permutation}",
            "type": "social"
        },
        "tiktok": {
            "rate_limit": 1500,
            "format": "https://www.tiktok.com/@{permutation}?",
            "type": "social"
        },
        "instagram": {
            "rate_limit": 1000,
            "format": "https://instagram.com/{permutation}",
            "type": "social"
        },
        "soundcloud": {
            "rate_limit": 1000,
            "format": "https://soundcloud.com/{permutation}",
            "type": "music"
        },
        "github": {
            "rate_limit": 1000,
